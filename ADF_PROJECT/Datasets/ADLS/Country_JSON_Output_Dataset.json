{
  "name": "Country_JSON_Output_Dataset",
  "properties": {
    "linkedServiceName": {
      "referenceName": "ADLS_LinkedService",
      "type": "LinkedServiceReference"
    },
    "parameters": {
      "CountryName": {
        "type": "String"
      }
    },
    "type": "Json",
    "typeProperties": {
      "location": {
        "type": "AzureBlobFSLocation",
        "fileName": {
          "value": "@{dataset().CountryName}.json",
          "type": "Expression"
        },
        "folderPath": "country-data"
      }
    }
  }
}
